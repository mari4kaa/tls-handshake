    "devDependencies": {
      "@nestjs/cli": "^11.0.14",
      "@nestjs/schematics": "^11.0.9",
      "@nestjs/testing": "^11.1.9",
      "@types/node": "^24.10.1",
      "ts-node": "^10.9.2",
      "typescript": "^5.9.3"
